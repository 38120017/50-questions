from datetime import date  
a = input().split("-" ) 
c =""
#rint (a) 
b = a[2].split("#")
#rint (b)
d = date(year=int(a[0]),month=int(a[1]),day=int(b[0]))
d1 = date(year=int(b[1]),month=int(a[-2]),day=int(a[-1]))
d2 = d-d1
ou = abs(d2)
print (ou)
for i in str(ou):
    if i.isdigit():
       c+=i
    else:
       break 
print (c)
